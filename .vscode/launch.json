{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Observador Local Debug",
      "request": "launch",
      "cwd": "${workspaceRoot}/modules/observador",
      "console": "internalConsole",
      "stopOnEntry": false,
      "mainClass": "com.exemplo.iot.App",
      "args": "",
      "projectName": "observador",
      "env": {
        "EdgeHubConnectionString": "${config:azure-iot-edge.EdgeHubConnectionString}",
        "EdgeModuleCACertificateFile": "${config:azure-iot-edge.EdgeModuleCACertificateFile}"
      }
    },
    {
      "type": "java",
      "name": "Observador Remote Debug",
      "request": "attach",
      "hostName": "localhost",
      "port": 5005
    }
  ]
}